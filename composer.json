{
    "name": "akrabat/slim3-skeleton",
    "description": "Simple Slim Framework 3 skeleton with Twig & Monolog.",
    "license": "BSD-3-Clause",
    "type": "project",
    "keywords": ["slim-framework", "skeleton"],
    "authors": [
        {
            "name": "Rob Allen",
            "email": "rob@akrabat.com",
            "homepage": "http://akrabat.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "require": {
        "slim/slim": "^3.0",
        "slim/twig-view": "^2.0",
        "slim/flash": "^0.1.0",
        "monolog/monolog": "^1.13",
        "vlucas/phpdotenv": "^2.2",
        "slim/pdo": "^1.9",
        "swiftmailer/swiftmailer": "^5.4",
        "codeguy/upload": "^1.3",
        "robmorgan/phinx": "^0.5.3",
        "jamosaur/randstring": "^1.2",
        "respect/validation": "^1.0",
        "phpunit/phpunit": "^5.3",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/dbunit": "^2.0"
    }
}
